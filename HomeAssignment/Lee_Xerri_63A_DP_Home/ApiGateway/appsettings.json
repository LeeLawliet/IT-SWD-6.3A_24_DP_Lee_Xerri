{
  "ReverseProxy": {
    "Routes": {
      "user-login": {
        "ClusterId": "userCluster",
        "Match": { "Path": "/api/user/login" },
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "{Authorization}"
          }
        ]
      },
      "user-register": {
        "ClusterId": "userCluster",
        "Match": { "Path": "/api/user/register" },
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "{Authorization}"
          }
        ]
      },
      "users": {
        "ClusterId": "userCluster",
        "Match": { "Path": "/api/user/{**catch-all}" },
        "Transforms": [
          {
            "RequestHeader": "Authorization",
            "Set": "{Authorization}"
          }
        ]
      },
      "bookings": {
        "ClusterId": "bookingCluster",
        "Match": { "Path": "/api/booking/{**catch-all}" }
      },
      "payments": {
        "ClusterId": "paymentCluster",
        "Match": { "Path": "/api/payment/{**catch-all}" }
      },
      "locations": {
        "ClusterId": "locationCluster",
        "Match": { "Path": "/api/location/{**catch-all}" }
      }
    },
    "Clusters": {
      "bookingCluster": {
        "Destinations": {
          "dest1": { "Address": "http://localhost:5238" }
        }
      },
      "paymentCluster": {
        "Destinations": {
          "dest1": { "Address": "http://localhost:5107" }
        }
      },
      "userCluster": {
        "Destinations": {
          "dest1": { "Address": "http://localhost:5018" }
        }
      },
      "locationCluster": {
        "Destinations": {
          "dest1": { "Address": "http://localhost:5292" }
        }
      }
    }
  }
}